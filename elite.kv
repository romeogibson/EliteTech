#Welcome Screen
<MainWindow>:
    name: "start"
    Image:
        source: "sunset.jpeg"
        size_hint: (1, 1)
        opacity: .8
        pos: 0,80
    Label:
        text: "Elite-Tech Troubleshooter"
        halign: "center"
        pos: 20,300
        font_size: 100
        #underline: True
        bold: True
        color: 0,0,0,1
        font_name: "NIAGSOL"
    Label:
        text: "\"Fixing internet issues, simply.\""
        halign: "center"
        pos: 20,250
        font_size: 20
        bold: True
        color: 0,0,0,1

    Button:
        text: "Get Started"
        size_hint: (.5, None)
        pos: 325, 330
        halign: "center"
        font_size: 46
        background_color: 1,0,1,1
        padding: 10,10
        font_name: "DejaVuSans"
        on_release:
            app.root.current= "hsiTrP"
            app.root.transition = SlideTransition(direction= "left")

    Label:
        text: "Developed by Romeo Gibson, 2020"
        color: 0,0,0,1
        font_size: 10
        font_name: "DejaVuSans"
        bold: True
        pos: 50, -320


########My Little Helper - to be removed at completion
    #Button:
        #text: "My little helper"
        #size_hint: (.3, .2)
        #pos: 0, 30
        #on_release:
            #app.root.current= "aftermboot"





<SecondWindow>:
    name: "hsiTrP"
    Image:
        source: "wifi_icon.png"
        size_hint:(.2,.1)
        pos: 500, 450
    Image:
        source: "sunset.jpeg"
        size_hint: (1, 1)
        opacity: .8
        pos: 0,80
    Label:
        text: "We're sorry that you're having issues with your internet.\nLuckily, we're here to help!"
        color: 0,0,0,1
        halign: "center"
        pos: 40, 50
        font_size: 24
        font_name: "DejaVuSans"

    Button:
        text: "Tap Here to Begin Troubleshooting"
        pos: 300,100
        size_hint: (.5, None)
        background_color: 0,1,1,1
        on_release:
            app.root.current= "ident"

    BackButton:
        source: "back.png"
        size_hint: (.1,.1)
        pos: -20, 600
        on_release:
            app.root.current= "start"


#Gathers info on the cust. PC - modem, modem - router, or modem-router combinations, etc.
<identifyWindow>:
    name: "ident"

    Label:
        text: "First, we need to determine your current setup."
        color: 0,0,0,1
        font_size: 24
        pos: 50,300
        halign: "center"


    Image:
        source: "modem_1.png"
        size_hint:(.8,.4)
        pos: 200, 300

    Label:
        text: "The easiest way to determine how your network is set up, is to look at the devices!\nIf you have two boxes connected to each other via ethernet, and see the COAX connection on one of those boxes,\nthis means you have a cable modem. However, your modem may only have a single ETHERNET port."
        color: 0,0,0,1
        font_size: 18
        pos: 50, -100
        halign: "center"
    Label:
        text: "hint: The COAX connection is identical to the same cord you would find on a Cable TV receiver"
        font_size: 12
        color: 1,0,0,1
        pos: 50, -140

    Button:
        text: "This is what I have"
        size_hint:(.4, .1)
        background_color: 0,1,1,1
        pos: 400, 120
        on_release:
            app.root.current= "rout"

    Button:
        text: "I don't have this device"
        size_hint: (.4, .1)
        background_color: 0,1,1,1
        pos: 400,40
        on_release:
            app.root.current= "trans"

    BackButton:
        source: "back.png"
        size_hint: (.1,.1)
        pos: -20, 600
        on_release:
            app.root.current= "hsiTrP"


<wifi>:
    name: "rout"
    Label:
        text: "Great! Now, we need to identify what is connected to your Cable Modem"
        color: 0,0,0,1
        halign: "center"
        pos: 50,300
        font_size: 16
        bold: True

    Image:
        source: "modem_1.png"
        size_hint:(.8,.4)
        pos: 200, 300

    Label:
        text: "Next, we need to verify what is connected to your MODEM via the ETHERNET port.\nKeep in mind, your modem may have less ethernet ports than shown in the image above."
        color: 0,0,0,1
        pos: 50, -90
    Label:
        text: "First, look on the back of your modem. You should see an ethernet cable connected (it resembles a phone line and can sometimes be yellow or blue in color)"
        color: 0,0,0,1
        pos: 50, -130
#Connected to PC
    Button:
        text: "My Modem is connected directly to my computer"
        background_color: 0,1,1,1
        size_hint: (.4,.1)
        pos: 150, 100
        on_release:
            app.root.current= "pc1"
#connected to Router
    Button:
        text: "My modem is connected to a similar device (router)"
        background_color: 0,1,1,1
        size_hint: (.4,.1)
        pos: 635, 100
        on_release:
            app.root.current= "hsiTr"
#Connected to nothing
    Button:
        text: "My modem isn't connected to anything."
        background_color: 0,1,1,1
        size_hint: (.4,.1)
        pos: 400, 10
        on_release:
            app.root.current= "trans"

    BackButton:
        source: "back.png"
        size_hint: (.1,.1)
        pos: -20, 600
        on_release:
            app.root.current= "ident"


<ThirdWindow>:
    name: "hsiTr"
    Label:
        text: "Ok, let's get started!"
        color: 0,0,0,1
        font_size: 32
        pos: 50,300
        halign: "center"

    Label:
        text: "We'll start by 'power-cycling' your 'router' "
        color: 0,0,0,1
        font_size: 24
        halign: "center"
        pos: 50, 200

    Image:
        source: "ethernet_port.png"
        halign: "center"
        pos: 180, 100
        size_hint: (.8,.6)


    Label:
        text: "Take a look at the diagram above. Don't worry, it'll also be on the next step!"
        color: 0,0,0,1
        font_size: 18
        pos: 50, -250

    Button:
        text: "Next"
        background_color: 0,1,1,1
        size_hint:(.1,.1)
        pos: 590, 10
        on_release:
            app.root.current= "r1"

    BackButton:
        source: "back.png"
        size_hint: (.1,.1)
        pos: -20, 600
        on_release:
            app.root.current= "start"

#Tier 1 Router Troubleshooting
<FourthWindow>:
    name: "r1"

    Label:
        text: "Keep in mind: Your 'router' may not resemble the image shown, however, the ports will likely be the same"
        color: 0,0,0,1
        pos: 110, -65
        halign: "center"
        color: 1,0,0,1

    Image:
        source: "ethernet_port.png"
        halign: "center"
        pos: 180, 280
        size_hint: (.8,.6)

    Label:
        text: "How to power-cycle your router:"
        color: 0,0,0,1
        halign: "center"
        pos: 110, -90
        bold: True
        underline: True

    Label:
        text: "Step 1: unplug the POWER CABLE from the POWER PORT \nStep 2: Wait 10 seconds\nStep 3: Plug the POWER CABLE into the POWER PORT of the router"
        color: 0,0,0,1
        halign: "left"
        pos: 110, -135
        font_size: 20

    Label:
        text: "Note: If the device you unplugged has a 'screw-on' cable, that is the wrong device.\nLook for the device WITHOUT the 'screw-on' cable."
        color: 1,0,0,1
        halign: "center"
        pos: 110, -200

    Label:
        text: "After plugging in your router, wait for several minutes until the lights stabilize.\nOnce stabilized, try reconnecting to your wifi."
        color: 0,0,0,1
        halign: "center"
        pos: 110, -250
        font_size: 22

    Button:
        text: "Continue Troubleshooting/My Issue wasn't resolved"
        size_hint:(.4, .1)
        height: 5
        pos: 450, 25
        background_color: 0,1,1,1
        on_release:
            app.root.current= "r2"

    BackButton:
        source: "back.png"
        size_hint: (.1,.1)
        pos: -20, 600
        on_release:
            app.root.current= "hsiTr"

#Tier 2 Troubleshooting, Modem restart is introduced.
<FifthWindow>:
    name: "r2"
    Label:
        text: "No worries! We'll try something else."
        color: 0,0,0,1
        font_size: 22
        font_name: "DejaVuSans"
        pos: 50, 300
        bold: True

    Label:
        text: "Let's start by unplugging the POWER CABLE from the ROUTER again. This time, leave it unplugged.\nNext, unplug the POWER CABLE from the MODEM, and wait 15 seconds."
        color: 0,0,0,1
        font_size: 20
        font_name: "DejaVuSans"
        pos: 50, 250

    Image:
        source: "clock.jpg"
        size_hint: (.3,.3)
        pos: 450,350

    Label:
        text: "Now, plug in the modem. Once the lights on the modem stabilize ('online' is a solid color)\nPlug the POWER CABLE into the POWER PORT of the router.\nOnce your router has rebooted, try reconnecting!"
        color: 0,0,0,1
        halign: "center"
        font_size: 24
        bold: True
        pos: 50, -30

    Button:
        text: "My issue has been resolved"
        size_hint: (.3,.1)
        pos: 280,100
        background_color: 0,1,1,1
        on_release:
            app.root.current= "start"


    Button:
        text: "My issue has NOT been resolved"
        size_hint: (.3,.1)
        pos: 640, 100
        background_color: 0,1,1,1
        color: 1,0,0,1
        on_release:
            app.root.current= "nores"

    BackButton:
        source: "back.png"
        size_hint: (.1,.1)
        pos: -20, 600
        on_release:
            app.root.current= "r1"

#When certain issues occur that can't be helped via an application, live support is necessary (cust can't find equipment
#-- , the device has been unplugged and cust can't reconnect on their own, etc.) Button is provided to help the customer
#---, contact their ISP to resolve the issue
<transfer>:
    name: "trans"
    Image:
        source: "sunset.jpeg"
        size_hint: (1, 1)
        opacity: .8
        pos: 0,80
    Label:
        text: "Sorry, it seems your issue is beyond our scope of support.\nTo resolve your issue, please contact your Internet Service Provider (ISP)."
        color: 0,0,0,1
        font_size: 24
        halign: "center"
        pos: 50,300
        bold: True

    Label:
        text: "As our last efforts to help, we've provided a link \nto find your ISP's support number if you don't already have it:"
        color: 0,0,0,1
        font_size: 16
        halign: "center"
        pos: 50, 200

    Button:
        text: "Find My ISP"
        background_color: 0,1,1,1
        halign: "center"
        size_hint: (.4,.4)
        pos: 400, 100
        font_size: 28
        on_release:
            webbrowser.open("https://broadbandnow.com/")

    BackButton:
        source: "back.png"
        size_hint: (.1,.1)
        pos: -20, 600
        on_release:
            app.root.current= "start"



